<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean id="iconitoPeriscolaireExternalService" parent="externalProviderServiceAdapter"
    class="fr.capwebct.capdemat.plugins.externalservices.iconitoperiscolaire.service.IconitoPeriscolaireService">
    <property name="label" value="ICONITO - PÃ©riscolaire"></property>
    <property name="iconitoPeriscolaireServiceClient" ref="iconitoPeriscolaireServiceClient" />
  </bean>

  <bean id="iconitoPeriscolaireServiceClient"
    class="fr.capwebct.capdemat.plugins.externalservices.iconitoperiscolaire.service.IconitoPeriscolaireServiceClient">
    <property name="urls">
      <map>
        <entry key="sendRequest"
          value="${plugins.externalservices.iconitoperiscolaire.send_request_uri}" />
        <entry key="checkExternalReferential"
          value="${plugins.externalservices.iconitoperiscolaire.check_external_referential_uri}" />
        <entry key="loadExternalInformation"
          value="${plugins.externalservices.iconitoperiscolaire.load_external_information_uri}" />
        <entry key="getFamilyAccounts"
          value="${plugins.externalservices.iconitoperiscolaire.get_family_accounts_uri}" />
        <entry key="loadInvoiceDetails"
          value="${plugins.externalservices.iconitoperiscolaire.load_invoice_details_uri}" />
        <entry key="loadAccountDetails"
          value="${plugins.externalservices.iconitoperiscolaire.load_account_details_uri}" />
        <entry key="creditAccount"
          value="${plugins.externalservices.iconitoperiscolaire.credit_account_uri}" />
        <entry key="getConsumptions"
          value="${plugins.externalservices.iconitoperiscolaire.get_consumptions_uri}" />
      </map>
    </property>
  </bean>
</beans>

