						<condition name="isCollectivity" type="trigger"/>
						<condition name="forAll" type="trigger" />
						<condition name="forContractorCollectivity" type="trigger" />
					</conditions>
                  </common>
                  <fo> 
					<!-- <radio /> -->
				  </fo>
                  <bo />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

