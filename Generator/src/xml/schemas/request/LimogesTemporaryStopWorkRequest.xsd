  <xs:element name="LimogesTemporaryStopWorkRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
       	Demande d'arrêté temporaire pour des travaux
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
		Temporary stop for work request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
       	Demande d'arrêté temporaire pour des travaux
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
		Temporary stop for work request
      </xs:documentation>
      <xs:appinfo>
        <model namespace="urbanism" />
        <common>
          <namespace name="urbanism" />
          <steps>
			<step index="0" name="requester">
				<conditions>
					<condition name="isLandlord" />
					<condition name="isContractor" />
					<condition name="isCollectivity" />
					<condition name="forAll" />
					<condition name="forContractorCollectivity" />
				</conditions>
			</step>
            <step index="1" name="work" >
				<conditions>
					<condition name="isExtension" />
				</conditions>
			</step>	
            <step index="2" name="informations">
				<conditions>
					<condition name="noParking" />
					<condition name="direction" />
					<condition name="alternate" />
					<condition name="ban" />
					<condition name="banBetween" />
					<condition name="deviation" />
				</conditions>
			</step>
            <step index="3" name="rules" />
		    <step index="4"  ref="document" />
            <step index="5" ref="validation" />
          </steps>
        </common>
      </xs:appinfo>
    </xs:annotation>

    <xs:complexType> 
      <xs:complexContent> 
        <xs:extension base="cvq:RequestType"> 
          <xs:sequence>

			<!-- Etape 0 : Demandeur -->
