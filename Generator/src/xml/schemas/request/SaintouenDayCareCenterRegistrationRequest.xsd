<xs:schema targetNamespace="http://www.cg95.fr/cvq/schema/sdccrr" 
           xmlns:sdccrr="http://www.cg95.fr/cvq/schema/sdccrr"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" 
           xmlns:cvq="http://www.cg95.fr/cvq/schema/common"
           elementFormDefault="qualified" 
           attributeFormDefault="unqualified">

  <xs:import namespace="http://www.cg95.fr/cvq/schema/common" schemaLocation="../common/CVQCommonElements.xsd" />

  <xs:element name="SaintouenDayCareCenterRegistrationRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande de pré-inscription en
        multi-accueil
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Pré-inscription en multi-accueil
      </xs:documentation>
      <xs:appinfo>
        <model namespace="babyhood" />
        <common>
          <namespace name="babyhood" />
          <steps>
            <step index="-1" ref="homeFolder" />
            <step index="0" name="subject">
              <widgets>
                <subject />
              </widgets>
              <conditions>
                <condition name="estAutreSituationActuelleMere" />
                <condition name="estAutreSituationActuellePere" />
                <condition name="estHorairesReguliersMere" />
                <condition name="estHorairesReguliersPere" />
              </conditions>
            </step>
            <step index="1" name="accueil">
              <conditions>
                <condition name="dateConnue" />
                <condition name="estHorairesAccueilRegulier" />
                <condition name="estHorairesAccueilIrregulier" />
              </conditions>
            </step>
            <step index="2" name="rendezVous" />
            <step index="3" ref="document" />
            <step index="4" ref="validation" />
          </steps>
        </common>
      </xs:appinfo>
    </xs:annotation>

    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>

            <!--        STEP 0 : SUBJECT       -->
            
            <xs:element name="DixHuitMoisEnfant" type="xs:date" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  18 mois de l'enfant
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  18 mois de l'enfant
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <bo column="2" />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="InformationMere" type="sdccrr:SdccrrMereType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Informations complémentaires
                  sur la mère
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Informations complémentaires sur la mère
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="InformationPere" type="sdccrr:SdccrrPereType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Informations complémentaires
                  sur le père
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Informations complémentaires sur le père
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <!--        STEP 1 : ACCUEIL         -->
            <xs:element name="DatePlacementAccueilRegulier" type="sdccrr:SdccrrDatesPlacementType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Date du placement
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Date du placement
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="ChoixHorairesAccueil" type="sdccrr:SdccrrChoixHorairesAccueilType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Vous souhaitez préciser des horaires d'accueil
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Vous souhaitez préciser des horaires d'accueil
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilRegulier" type="trigger"/>
                        <condition name="estHorairesAccueilIrregulier" type="trigger"/>
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="PlageHoraireAccueilReguliere" type="sdccrr:SdccrrPlageHoraireType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Horaire d'accueil pour tous les jours de la semaine
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Horaire d'accueil pour tous les jours de la semaine
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilRegulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Lundi" type="sdccrr:SdccrrPlageHoraireLundiType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Lundi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Lundi
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Mardi" type="sdccrr:SdccrrPlageHoraireMardiType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Mardi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Mardi
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Mercredi" type="sdccrr:SdccrrPlageHoraireMercrediType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Mercredi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Mercredi
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Jeudi" type="sdccrr:SdccrrPlageHoraireJeudiType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Jeudi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Jeudi
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Vendredi" type="sdccrr:SdccrrPlageHoraireVendrediType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Vendredi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Vendredi
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <!--        STEP 2 : Prise de rendez-vous         -->
            <xs:element name="ChoixTypeRendezVous" type="sdccrr:SdccrrRendezVousType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Vous préférez un rendez-vous :
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Vous préférez un rendez-vous :
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="TelephoneContact" type="cvq:PhoneType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  N° de téléphone sur lequel
                  nous vous contacterons
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  N° de téléphone sur lequel nous vous contacterons
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="PlageHoraireContact" type="cvq:LocalReferentialDataType" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Moment de la journée où vous préférez que nous vous contactions
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Plages horaires
                </xs:documentation>
                <xs:appinfo>
                  <model namespace="request" isTiedToRequest="true" />
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="CommentaireCitoyen" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Renseignements
                  supplémentaires
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Renseignements supplémentaires
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo>
                    <textarea rows="10" />
                  </fo>
                  <bo column="2">
                    <textarea rows="10" />
                  </bo>
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="600" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!--  types complexes -->
